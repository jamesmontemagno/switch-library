{
  "TheGamesDB": {
    "ApiKey": "YOUR_API_KEY_HERE",
    "BaseUrl": "https://api.thegamesdb.net/",
    "Version": 1
  },
  "BlobStorage": {
    "ConnectionString": "YOUR_CONNECTION_STRING_HERE_OR_UseDevelopmentStorage=true",
    "ContainerName": "games-cache"
  },
  "SqlDatabase": {
    "ConnectionString": "Server=tcp:YOUR_SERVER.database.windows.net,1433;Initial Catalog=switchlibrary-games;User ID=YOUR_USERNAME;Password=YOUR_PASSWORD;Encrypt=True;TrustServerCertificate=False;",
    "DatabaseName": "switchlibrary-games",
    "CommandTimeout": 30,
    "EnableRetryOnFailure": true
  },
  "StorageMode": "Blob",
  "Platforms": {
    "NintendoSwitch": 4971,
    "NintendoSwitch2": 5021
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}
