{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "timers": {
            "enableDistributedTracing": true
        }
    },
    "functionTimeout": "00:30:00",
    "retry": {
        "strategy": "exponentialBackoff",
        "maxRetryCount": 3,
        "minimumInterval": "00:00:05",
        "maximumInterval": "00:05:00"
    }
}